{
    "name": "microgen",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "graphql": "node ./dist/graphql",
        "build": "NODE_ENV=production babel graphql.js config.js monitor.js -d dist && NODE_ENV=production babel graphql/ -d dist/graphql && NODE_ENV=production babel utils/ -d dist/utils",
        "dev": "npm-run-all --parallel graphql-dev email-services-dev pushNotification-services-dev storage-services-dev user-services-dev workspace-services-dev project-services-dev server-services-dev",
        "serve": "node graphql",
        "test": "NODE_ENV=test nyc mocha --require babel-core/register test/test_helper.js './src/**/*.spec.js' --exit --timeout 20000",
        "user-services": "cd ./services/user && node index.js",
        "email-services": "cd ./services/email && node index.js",
        "storage-services": "cd ./services/storage && node index.js",
        "pushNotification-services": "cd ./services/push-notification && node index.js",
        "graphql-dev": "nodemon --exec babel-node graphql",
        "user-services-dev": "cd ./services/user && nodemon index.js",
        "email-services-dev": "cd ./services/email && nodemon index.js",
        "storage-services-dev": "cd ./services/storage && nodemon index.js",
        "pushNotification-services-dev": "cd ./services/push-notification && nodemon index.js",
        "workspace-services": "cd ./services/workspace && node index.js",
        "workspace-services-dev": "cd ./services/workspace && nodemon index.js",
        "project-services": "cd ./services/project && node index.js",
        "project-services-dev": "cd ./services/project && nodemon index.js",
        "server-services": "cd ./services/server && node index.js",
        "server-services-dev": "cd ./services/server && nodemon index.js",
        "start": "npm run-script build && npm-run-all --parallel graphql email-services pushNotification-services storage-services user-services workspace-services project-services server-services"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@feathersjs/authentication": "^4.3.0",
        "@feathersjs/authentication-local": "^4.3.0",
        "@feathersjs/authentication-oauth": "^4.3.0",
        "@feathersjs/configuration": "^4.3.0",
        "@feathersjs/errors": "^4.3.0",
        "@feathersjs/express": "^4.3.0",
        "@feathersjs/feathers": "^4.3.0",
        "@feathersjs/socketio": "^4.3.0",
        "@sendgrid/mail": "^6.4.0",
        "apollo-server": "^2.9.0",
        "app-root-path": "^2.2.1",
        "graphql-rate-limit-directive": "^1.1.0",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "aws-sdk": "^2.529.0",
        "axios": "^0.19.0",
        "cote": "https://github.com/Rekeningku/cote",
        "express": "^4.17.1",
        "dotenv": "^8.1.0",
        "bcryptjs": "^2.4.3",
        "feathers-mongoose": "^8.0.2",
        "feathers-permissions": "^0.2.1",
        "google-auth-library": "^5.2.2",
        "graphql": "^14.5.3",
        "graphql-subscriptions": "^1.1.0",
        "graphql-type-json": "^0.3.0",
        "helmet": "^3.20.0",
        "lodash": "^4.17.15",
        "mongodb-core": "^3.2.7",
        "mongoose": "^5.6.11",
        "mongoose-lean-virtuals": "^0.4.3",
        "response-time": "^2.3.2",
        "serve-favicon": "^2.5.0",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-plugin-module-resolver": "^3.0.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.1",
        "npm-run-all": "^4.1.5"
    }
}
